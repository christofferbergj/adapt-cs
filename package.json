  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
